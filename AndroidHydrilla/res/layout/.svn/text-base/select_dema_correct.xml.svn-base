<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
     xmlns:custom="http://schemas.android.com/apk/res/cn.cowis.hydrilla.app"
           android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" 
            >

            <TextView
               
                android:text="标定操作"
                style="@style/classify"
               />

          
            <!--  -->

            <LinearLayout
                
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_10"
                android:layout_marginLeft="@dimen/margin_10"
            	android:layout_marginRight="@dimen/margin_10"
                 android:background="@drawable/bg_selector_shape_rect_layout"
                android:orientation="vertical" >
                
                <cn.cowis.hydrilla.view.CustomLinearLayoutButton
                    android:id="@+id/elec_correct_layout_1"
                    android:background="@drawable/bg_selector_nostroke_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    custom:buttonName="@string/dema_elec_param"
                    >
                    
                </cn.cowis.hydrilla.view.CustomLinearLayoutButton>
                
                
                <TextView
                   style="@style/line"/>
                
                
                
                  <cn.cowis.hydrilla.view.CustomLinearLayoutButton
                    android:id="@+id/elec_correct_layout_2"
                    android:background="@drawable/bg_selector_nostroke_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    custom:buttonName="@string/correct_rest_elec"
                    >
                    
                </cn.cowis.hydrilla.view.CustomLinearLayoutButton>
              
                
               
            </LinearLayout>

        </LinearLayout>