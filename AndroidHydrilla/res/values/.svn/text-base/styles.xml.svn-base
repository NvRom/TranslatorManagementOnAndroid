<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="@style/Theme.Sherlock.Light.DarkActionBar">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
        
         <item name="android:textSize">15sp</item>
        
         <item name="android:actionMenuTextColor">@color/white</item>
    </style>
    
    
   <style name="MenuTextStyle">
    <item name="android:textColor">@android:color/holo_blue_bright</item>
  </style>
    <!-- 后面的分类
    -->
    
    <style name="classify">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/margin_15</item>
        <item name="android:background">@color/gray</item>
        <item name="android:paddingLeft">@dimen/margin_5</item>
        <item name="android:textSize">@dimen/view_size_middle</item>
        
    </style>
    
    
    <!-- 一条细线 -->
    
    <style name="line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">#C0C0C0</item>
    </style>
    
    
    <!-- LinearLayout 的按钮 -->
    <style name="linearLayoutStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/bg_selector_shape_rect_layout</item>
    </style>
    
    
    <!-- 后面的-->
      <!-- 一条 浅色的底线 白色 -->
    <style name="botton_line">
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">#DCDCDC</item>
    </style>
    
    <!-- edit的类型 -->
    <style
        name="edit_style"
        >
        <item name="android:background">@drawable/bg_selector_shape_oblong_edit</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:gravity">center</item>
        
    </style>
    <!-- 一个 标记< -->
    <style
        name="mark"
        >
        <item name="android:text">@string/mark</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">5dp</item>
        
    </style>
    
    <!--
     android:layout_marginTop="@dimen/margin_5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal" 
     -->
    <style 
        name="parent_mark"
        >
        
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
     
    </style>

    <style 
        name="textStyle">
         <item name ="android:layout_width">wrap_content</item> 
         <item name ="android:layout_height">@dimen/view_size_superbig</item> 
         <item name ="android:layout_marginLeft">@dimen/margin_10</item>                     
         <item name ="android:textColor">#000</item>                    
         <item name ="android:gravity">center_vertical</item>  
          
    </style>
    
    <style 
        name="numberStyle">
         <item name ="android:layout_width">wrap_content</item> 
         <item name ="android:layout_height">30dp</item> 
         <item name ="android:layout_marginTop">2px</item> 
         <item name ="android:layout_marginRight">2px</item>  
         <item name ="android:layout_marginBottom">2px</item>                  
                           
         <item name ="android:gravity">center</item>  
         <item name ="android:inputType">number</item>
         <item name ="android:selectAllOnFocus">true</item>
         <item name ="android:background">@drawable/bg_selector_shape_rect_edit</item>
      
         <item name ="android:ems">3</item>
          
          
    </style>
    
    <style 
        name="numberSignedStyle">
         <item name ="android:layout_width">wrap_content</item> 
         <item name ="android:layout_height">30dp</item> 
         <item name ="android:layout_marginTop">2px</item> 
         <item name ="android:layout_marginRight">2px</item>  
         <item name ="android:layout_marginBottom">2px</item>                  
                           
         <item name ="android:gravity">center</item>  
         <item name ="android:inputType">numberSigned</item>
         <item name ="android:selectAllOnFocus">true</item>
         <item name ="android:background">@drawable/bg_selector_shape_rect_edit</item>
         
         <item name ="android:ems">6</item>
     
    </style>
    
    <style 
        name="numberDecimalStyle">
         <item name ="android:layout_width">wrap_content</item> 
         <item name ="android:layout_height">30dp</item> 
         <item name ="android:layout_marginTop">2px</item> 
         <item name ="android:layout_marginRight">2px</item>  
         <item name ="android:layout_marginBottom">2px</item>                  
                           
         <item name ="android:gravity">center</item>  
         <item name ="android:inputType">numberDecimal</item>
         <item name ="android:selectAllOnFocus">true</item>
         <item name ="android:background">@drawable/bg_selector_shape_rect_edit</item>
        
         <item name ="android:ems">5</item>
     
    </style>
    
     <style 
        name="numberSingedDecimalStyle">
         <item name ="android:layout_width">wrap_content</item> 
         <item name ="android:layout_height">30dp</item> 
         <item name ="android:layout_marginTop">2px</item> 
         <item name ="android:layout_marginRight">2px</item>  
         <item name ="android:layout_marginBottom">2px</item>                  
                           
         <item name ="android:gravity">center</item>  
         <item name ="android:inputType">numberDecimal</item>
         <item name ="android:digits">1234567890.+-</item>
         <item name ="android:selectAllOnFocus">true</item>
         <item name ="android:background">@drawable/bg_selector_shape_rect_edit</item>
       
         <item name ="android:ems">5</item>
     
    </style>
    
    <style 
        name="numberSingedDecimalStyle_NH4">
         <item name ="android:layout_width">0dp</item> 
         <item name ="android:layout_height">match_parent</item>
         <item name ="android:layout_weight">3</item> 
         <item name ="android:layout_marginTop">4dp</item> 
         <item name ="android:layout_marginRight">2dp</item>  
         <item name ="android:layout_marginBottom">4dp</item>                  
                           
         <item name ="android:gravity">center</item>  
         <item name ="android:textSize">11sp</item>  
         <item name ="android:inputType">numberDecimal</item>
         <item name ="android:digits">1234567890.+-</item>
         <item name ="android:imeOptions">actionNext</item>
         <item name ="android:background">@drawable/bg_selector_shape_rect_edit</item>
    </style>
    
    <style 
        name="input_1">
         <item name ="android:layout_width">wrap_content</item> 
         <item name ="android:layout_height">@dimen/view_size_middle</item> 
         <item name ="android:layout_marginLeft">@dimen/view_size_small</item>                     
         <item name ="android:textColor">#000</item>                  
         <item name ="android:gravity">bottom</item>  
          
    </style>
    
     <style 
        name="input_2">
         <item name ="android:layout_width">wrap_content</item> 
         <item name ="android:layout_height">wrap_content</item> 
         <item name ="android:textColor">#000</item>  
         <item name ="android:layout_marginLeft">@dimen/view_size_middle</item>                     
  
    </style>
    
      <style 
        name="margin_10">
     
         <item name ="android:layout_marginLeft">@dimen/view_size_small</item> 
         <item name ="android:layout_marginRight">@dimen/view_size_small</item>                     
  
    </style>
     
    <style name="CustomWindowTitleBackground"> 
    	<item name="android:background">#000</item> 
	</style>
	
	<style name="title" parent="android:Theme"> 
    	<item name="android:windowTitleSize">50dp</item> 
    	<item name="android:windowTitleBackgroundStyle">@style/CustomWindowTitleBackground</item>
	</style>
	
	<style name="afterconnection_title" parent="android:Theme"> 
    	<item name="android:windowTitleSize">60dp</item> 
    	<item name="android:windowTitleBackgroundStyle">@style/CustomWindowTitleBackground</item>
	</style>
	
	<style name="resultLayout"> 
    	<item name="android:layout_width">match_parent</item> 
    	<item name="android:layout_height">wrap_content</item> 
    	<item name="android:layout_marginLeft">@dimen/margin_10</item>
    	<item name="android:layout_marginRight">@dimen/margin_10</item>
    	<item name="android:layout_marginTop">@dimen/margin_10</item>
    	
    	<item name="android:orientation">vertical</item>	
	</style>

</resources>
